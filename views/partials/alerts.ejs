<% if(alerts.success){ %>
	<% alerts.success.forEach(function(msg){ %>
		<div>
			<%= msg %>
		</div>
	<% }) %>
<% } %>

<% if(alerts.error){ %>
	<% alerts.error.forEach(function(msg){ %>
		<div class="row">
			<div class="card-panel red lighten-1 white-text">
				<h5><%= msg %></h5>
			</div>
		</div>
	<% }) %>
<% } %>